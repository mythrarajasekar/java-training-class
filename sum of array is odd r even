class Array {
    static String array(int[] a) {
        int sum = 0;
        for (int i = 0; i < a.length; i++) {
            sum += a[i];
        }
        if (sum % 2 == 0)
            return "Even";
        else
            return "Odd";
    }
}

class Main {
    public static void main(String[] args) {
        int[] numbers = {1, 2, 3, 4, 5};
        Array ob = new Array();
        String result = ob.array(numbers);
        System.out.println(result);
        
    }
}

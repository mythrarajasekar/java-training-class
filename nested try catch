import java.util.*;

class Main {
    public static void main(String[] args) {
        // ArrayIndexOutOfBoundsException
        try {
             // ArithmeticException
            try {
                  div();
            } catch (ArithmeticException e) {
                  System.out.println("Arithmetic"); 
                  arr();
            }
        } catch (ArrayIndexOutOfBoundsException e) {
            System.out.println("Array index"); 
        }

        

        // NullPointerException
        try {
            // InputMismatchException
            try {
              inputs();
            } catch (InputMismatchException e) {
                  System.out.println("input mismatch");
                  length();
            }
        } catch (NullPointerException e) {
            System.out.println("Null pointer");
        }
    }
    
    public static void arr() throws ArrayIndexOutOfBoundsException{
        int[] a = new int[3];
        a[7] = 8;
    }
    
    public static void div() throws ArithmeticException{
        int b = 10 / 0;
        System.out.println(b);
    }
    
    public static void length() throws NullPointerException{
        String c = null;
        System.out.println(c.length());
    }
    
    public static void inputs() throws InputMismatchException{
        Scanner sc = new Scanner(System.in);
        int d = sc.nextInt();
        System.out.println(d);
    }
}

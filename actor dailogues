import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
        System.out.print("Enter the actor name: ");
        String actor=sc.nextLine();
        actfun(actor);
    }
    public static void actfun(String actor){
       if(actor.equals("vijay")) vijay();
       else if(actor.equals("ajith"))ajith();
       else if(actor.equals("kamal"))kamal();
       else System.out.println("try with some valid name");
    }
   static void vijay(){
        System.out.print("Vijay Iconic dialogues");
    }
    static void ajith(){
    System.out.print("Ajith Iconic dialogues");
}
     static void kamal(){
        System.out.print("Kamal Iconic dialogues");
    }
   
}
